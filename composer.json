{
  "name": "umbrella-evgeny-nefedkin/fb-lite",
  "description": "Lite bundle for authorization via facebook",
  "keywords": ["facebook", "security"],
  "type": "symfony-bundle",
  "license": "MIT",
  "version": "0.1-dev",
  "authors": [
    { "name": "Evgeny Nefedkin",
      "email": "evgeny.nefedkin@umbrellait.com"
    }
  ],

  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/umbrella-evgeny-nefedkin/FacebookLiteBundle.git"
    }
  ],

  "require": {
    "php": "^7.1.3",
    "facebook/graph-sdk": "^5.6"

  },
  "autoload": {
    "psr-4": {
      "FacebookLiteBundle\\": "src/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
