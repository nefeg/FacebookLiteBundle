{
  "name": "path-to-repo/fb-lite",
  "description": "Lite bundle for authorization via facebook",
  "keywords": ["facebook", "security"],
  "type": "symfony-bundle",
  "license": "MIT",
  "version": "0.1-dev",

  "repositories": [
    {
      "type": "vcs",
      "url": "set path to repo git here"
    }
  ],

  "require": {
    "php": "^7.1.3",
    "facebook/graph-sdk": "^5.6"

  },
  "autoload": {
    "psr-4": {
      "Aimchat\\FacebookLiteBundle\\": "src/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
